{
    "builders": [
        {
            "export_path": "image.tar",
            "image": "yikaus/alpine-bash",
            "type": "docker"
        }
    ],
    "provisioners": [
        {
            "type": "shell",
            "only": ["docker"],
            "script": "provision.sh"
        }
    ],
    "post-processors": [
        {
            "type": "docker-import",
            "only": ["docker"],
            "repository": "alligrader/golang-node",
            "tag": "latest"
        }
    ]
}
